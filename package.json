{
    "name": "reinstaller",
    "description": "A installing tool",
    "version": "1.0.0-alpha.2",
    "packageManager": "pnpm@7.17.0",
    "bin": {
        "reinstaller": "./lib/installer.mjs",
        "installer": "./lib/installer.mjs"
    },
    "main": "lib/index.cjs",
    "module": "lib/index.mjs",
    "exports": {
        ".": {
            "require": "./lib/index.cjs",
            "import": "./lib/index.mjs"
        },
        "./lib": "./lib/index.cjs",
        "./*": "./*"
    },
    "imports": {
        "#lib/*": {
            "require": "./lib/*.cjs",
            "import": "./lib/*.mjs"
        }
    },
    "files": [
        "es",
        "lib",
        "typings"
    ],
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src"
    },
    "scripts": {
        "build": "rimraf lib es && rollup -c",
        "build:types": "run-s build-temp-types roll-types",
        "build-temp-types": "tsc --emitDeclarationOnly --outDir temp/ -p src/",
        "patch-types": "node script/patchTypes",
        "roll-types": "api-extractor run && rimraf temp",
        "dev": "rollup -c -w",
        "pub": "tscjs scripts/publish",
        "unpub": "tscjs scripts/unpublish",
        "sync": "tscjs scripts/sync",
        "dist": "run-s eslint prettier build build:types docs prettier:docs",
        "test-unit": "jest --filter ./scripts/filter-unit.js",
        "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
        "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "docs": "rimraf docs && typedoc",
        "eslint": "eslint --fix .",
        "prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\"",
        "prettier:docs": "prettier --write \"**/*.md\""
    },
    "dependencies": {
        "@gitmars/core": "^3.3.5",
        "commander": "^9.4.1",
        "consola": "^2.15.3",
        "core-js": "^3.26.1",
        "fs-extra": "^10.1.0",
        "js-cool": "^2.5.0",
        "lodash-es": "^4.17.21",
        "lodash-unified": "^1.0.3",
        "ora": "^6.1.2",
        "os-locale": "^6.0.2",
        "picocolors": "^1.0.0",
        "shelljs": "^0.8.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.20.2",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.20.2",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@eslint-sets/eslint-config-simple-ts": "^3.0.5",
        "@microsoft/api-extractor": "^7.33.6",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^23.0.2",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^29.2.3",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^18.11.9",
        "@types/shelljs": "^0.8.11",
        "babel-loader": "^9.1.0",
        "coveralls": "^3.1.1",
        "cross-env": "^7.0.3",
        "esbuild": "^0.15.15",
        "eslint": "^8.28.0",
        "jest": "^29.3.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.0",
        "prettier-config-common": "^1.3.0",
        "rimraf": "^3.0.2",
        "rollup": "^3.4.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "rollup-plugin-visualizer": "^5.8.3",
        "ts-jest": "^29.0.3",
        "tsnd": "^1.1.0",
        "typedoc": "^0.23.21",
        "typedoc-plugin-markdown": "^3.13.6",
        "typescript": "^4.9.3"
    },
    "keywords": [
        "installer",
        "extension",
        "reinstaller",
        "saqqdy"
    ],
    "license": "MIT",
    "author": "saqqdy <https://github.com/saqqdy>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/reinstaller.git"
    },
    "bugs": {
        "url": "https://github.com/saqqdy/reinstaller/issues"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "tslib",
                "webpack"
            ],
            "allowedVersions": {
                "eslint": "^8.0.0"
            }
        }
    },
    "homepage": "https://github.com/saqqdy/reinstaller#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
}
