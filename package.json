{
	"name": "reinstaller",
	"description": "A installing tool",
	"version": "2.1.0",
	"packageManager": "pnpm@7.25.0",
	"bin": {
		"reinstaller": "./bin/index.js",
		"installer": "./bin/index.js"
	},
	"main": "./lib/installer.cjs",
	"module": "./es/installer.mjs",
	"typings": "./typings/installer.d.ts",
	"exports": {
		".": {
			"require": "./lib/index.cjs",
			"import": "./es/index.mjs"
		},
		"./*": "./*"
	},
	"files": [
		"bin",
		"dist",
		"es",
		"lib",
		"typings"
	],
	"directories": {
		"bin": "bin",
		"dist": "dist",
		"es": "es",
		"lib": "lib",
		"src": "src"
	},
	"scripts": {
		"build": "run-s clean build:{bundle,types} roll-types",
		"build:bundle": "rollup -c build/rollup.config.ts --configPlugin @rollup/plugin-typescript",
		"build:umd": "webpack --config build/webpack.config.js",
		"build:types": "tsc --emitDeclarationOnly --outDir temp -p src",
		"build:docs": "rimraf docs && typedoc && prettier --write \"**/*.md\"",
		"roll-types": "api-extractor run && rimraf temp",
		"watch": "pnpm build:bundle -w",
		"pub": "tscjs scripts/publish",
		"unpub": "tscjs scripts/unpublish",
		"sync": "tscjs scripts/sync",
		"clean": "rimraf lib es dist",
		"dist": "run-s clean build build:docs",
		"test-unit": "jest --filter ./scripts/filter-unit.js",
		"jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
		"coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"eslint": "eslint --fix .",
		"prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\""
	},
	"dependencies": {
		"@node-kit/utils": "^2.0.0",
		"commander": "^10.0.0",
		"consola": "^2.15.3",
		"core-js": "^3.27.2",
		"cosmiconfig": "^8.0.0",
		"debug": "^4.3.4",
		"global-modules": "^2.0.0",
		"js-cool": "^2.8.0",
		"monorepo-root": "^1.3.4",
		"ora": "^6.1.2",
		"preferred-pm": "^3.0.3",
		"workspace-projects": "^1.3.4"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@esbuild-kit/esm-loader": "^2.5.4",
		"@eslint-sets/eslint-config-ts": "^4.1.0",
		"@microsoft/api-extractor": "^7.34.0",
		"@rollup/plugin-alias": "^4.0.3",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.1",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-terser": "^0.4.0",
		"@rollup/plugin-typescript": "^11.0.0",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.11.18",
		"@types/shelljs": "^0.8.11",
		"babel-loader": "^9.1.2",
		"chalk": "^5.2.0",
		"coveralls": "^3.1.1",
		"cross-env": "^7.0.3",
		"esbuild": "^0.17.4",
		"eslint": "^8.32.0",
		"jest": "^29.4.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"prettier-config-common": "^1.3.0",
		"rimraf": "^4.1.2",
		"rollup": "^3.11.0",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-replace-shebang": "^1.2.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"ts-jest": "^29.0.5",
		"ts-morph": "^17.0.1",
		"tsnd": "^1.1.0",
		"typedoc": "^0.23.24",
		"typedoc-plugin-markdown": "^3.14.0",
		"typescript": "^4.9.4"
	},
	"peerDependencies": {
		"debug": ">= 4",
		"js-cool": ">= 2"
	},
	"keywords": [
		"installer",
		"extension",
		"reinstaller",
		"saqqdy"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/reinstaller.git"
	},
	"bugs": {
		"url": "https://github.com/saqqdy/reinstaller/issues"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"tslib",
				"webpack"
			],
			"allowedVersions": {
				"eslint": "^8.0.0"
			}
		}
	},
	"homepage": "https://github.com/saqqdy/reinstaller#readme",
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
